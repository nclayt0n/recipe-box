{
    "version": 2,
    "alias": "nrclayton-recipe-box",
    "name": "recipe-box",
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ],
    "build": {
        "env": {
            "REACT_APP_API_ENDPOINT": "https://nc-recipebox.herokuapp.com/api"
        }
    }
}